{
  "manifest_version": 3,
  "name": "Extractor de Elementos",
  "version": "1.5",
  "description": "Seleccionar un elemento.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_icon": "extractor.png",
    "default_title": "Action Button"
  },
  "icons": {
    "128": "extractor.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
